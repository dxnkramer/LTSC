<Configuration>
  <Add OfficeClientEdition="64" Channel="PerpetualVL2024" OfficeMgmtCOM="TRUE">

    <!-- Office Pro Plus 2024 -->
    <Product ID="ProPlus2024Volume">
      <Language ID="en-gb" />
      <Language ID="en-us" />
      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="OneDrive" />
    </Product>

    <!-- Visio Standard 2024 -->
    <Product ID="VisioStd2024Volume">
      <Language ID="en-gb" />
      <Language ID="en-us" />
      <ExcludeApp ID="Lync" />
