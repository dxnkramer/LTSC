    <!-- Visio Professional 2024 -->
    <Product ID="VisioPro2024Volume">
      <Language ID="en-gb" />
      <Language ID="en-us" />
      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="OneDrive" />
    </Product>

    <!-- Project Standard 2024 -->
    <Product ID="ProjectStd2024Volume">
      <Language ID="en-gb" />
      <Language ID="en-us" />
      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="OneDrive" />
    </Product>

    <!-- Project Professional 2024 -->
    <Product ID="ProjectPro2024Volume">
      <Language ID="en-gb" />
      <Language ID="en-us" />
      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="OneDrive" />
